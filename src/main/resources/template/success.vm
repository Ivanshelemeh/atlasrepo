#* @vtlvariable name="action" type="com.atlassian.shele.shelePlugin.webwork.MywebWorkAction" *#
#set($context=$action.servletContext)
#set($title="Oh comeon!")
#set($entity1=$entity.getAuthorIssue)
#set($AuthorIssue="Author Issue")
#set($time ="Time creation")
#set($fl = "List of fields")
#set($ch="New Field")
#set($prev="Previous Field")
#set($prodId="Id Project")
#set($workflow="Work flow")

<html>
<head>
    <meta name="decorator" content="atl.admin"/>
    <meta name="admin.active.section" content="jira.project.sidebar.navigation"/>
    <meta name="admin.active.tab" content="my-web-item"/>

    <title>$title</title>
</head>
<body>
<table class="aui">
    <thead name ="Dbraws">
    <tr>
        <th>$AuthorIssue</th>
        <th>$time</th>
        <th>$fl</th>
        <th>$ch</th>
        <th>$prev</th>
        <th>$prodId</th>
        <th>$workflow</th>
        <th>Event</th>
    </tr>
    </thead>
    <tbody>
        #foreach($entity in $context.getAttribute("entity"))
        <tr>
            #set($foo=$entity.getAuthorIssue())
            <td><strong>#if($foo)
                $foo
            #end</strong></td>
            #set($bar=$entity.getCreatedTime())

            <td><strong>#if($bar)
            $bar
#end</strong></td>
            #set($field=$entity.getField())
            <td><strong>#if($field)
            $field
#end</strong></td>
            #set($change=$entity.getNewField())
            <td><strong>#if($change)
            $change
#end</strong></td>
            #set($preChange=$entity.getPrevField())
            <td><strong>#if($preChange)
            $preChange
#end</strong></td>
            #set($prod=$entity.getProjectId())
            <td><strong>#if($prod)
            $prod
#end</strong></td>
            #set($flow=$entity.getWorkFlow())
            <td><strong>#if($flow)
            $flow
        #end
            </strong></td>
            #set($ev=$entity.getEvent())
            <td><strong>#if($ev)
                    $ev
                #end</strong></td>

        </tr>
        #end
    </tbody>
</table>
</body>
</html>